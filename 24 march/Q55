// 55. Write a C program that swaps two numbers without using a third variable.  
// Input value for x & y:
// Before swapping the value of x & y: 5 7
// After swapping the value of x & y: 7 5

#include <stdio.h>

int main() {
    int x, y;

    printf("Input value for x : ");
    scanf("%d", &x);
    printf("Input value for y : ");
    scanf("%d", &y);

    printf("Before swapping the value of x & y: %d %d\n", x, y);

    x = x + y;
    y = x - y;
    x = x - y;

    printf("After swapping the value of x & y: %d %d\n", x, y);

    return 0;
}
